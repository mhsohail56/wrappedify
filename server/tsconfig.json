{
  "compilerOptions": {
    "target": "es2016",
    "module": "Node16",
    "lib": ["dom", "es2022"],
    "outDir": "app",
    "rootDir": "./",
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true
  }
}
